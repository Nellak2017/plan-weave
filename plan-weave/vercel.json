{
	"version": 2,
	"github": {
    	"enabled": false
  	},
	  "builds": [
		{
		  "src": "plan-weave/**",
		  "use": "@vercel/next",
		  "config" : {
			"installCommand": "yarn set version berry && yarn plugin import workspace-tools && yarn config set nodeLinker node-modules && yarn install"
		  }
		}
	  ],
	  "routes": [
		{
		  "src": "/(.*)",
		  "dest": "/plan-weave/$1",
		  "headers": {
			"cache-control": "public, max-age=31536000, immutable"
		  }
		}
	  ]
}